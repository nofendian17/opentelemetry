{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 1,
  "graphTooltip": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Request Rate (req/s)",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "rate(http_server_requests_total[5m])",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate (5xx / s)",
      "id": 2,
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "rate(http_server_errors_total[5m])",
          "legendFormat": "errors/s"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Inflight Requests",
      "id": 3,
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(http_server_inflight_requests)"
        }
      ]
    },
    {
      "type": "stat",
      "title": "P95 Latency (s)",
      "id": 4,
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Duration Histogram (avg)",
      "id": 5,
      "gridPos": { "x": 0, "y": 4, "w": 10, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_server_request_duration_seconds_sum[5m]) / rate(http_server_request_duration_seconds_count[5m])",
          "legendFormat": "avg latency"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Route",
      "id": 6,
      "gridPos": { "x": 10, "y": 4, "w": 14, "h": 8 },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(http_server_requests_total[5m]))",
          "legendFormat": "{{http_route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Runtime: Goroutines",
      "id": 7,
      "gridPos": { "x": 0, "y": 12, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "go_runtime_go_goroutines"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Runtime: GC Pauses (s)",
      "id": 8,
      "gridPos": { "x": 8, "y": 12, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "rate(go_runtime_gc_pause_total_seconds_sum[5m])"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Runtime: Heap / Alloc (bytes)",
      "id": 9,
      "gridPos": { "x": 16, "y": 12, "w": 8, "h": 6 },
      "targets": [
        {
          "expr": "go_runtime_memory_heap_alloc_bytes",
          "legendFormat": "heap_alloc"
        }
      ]
    },
    {
      "type": "table",
      "title": "Errors by Route (5xx)",
      "id": 10,
      "gridPos": { "x": 0, "y": 18, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum by (http_route) (increase(http_server_errors_total[15m]))",
          "legendFormat": "{{http_route}}"
        }
      ]
    },
    {
      "type": "table",
      "title": "Latency Percentiles",
      "id": 11,
      "gridPos": { "x": 12, "y": 18, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Log Rate",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "expr": "sum by (level) (rate({job=\"promtail\"} | json [5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 13,
      "type": "logs",
      "title": "Recent Logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "expr": "{job=\"promtail\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 14,
      "type": "traces",
      "title": "Recent Traces",
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "nativeSearch",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["opentelemetry", "go", "demo"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Go Service Observability",
  "version": 2
}