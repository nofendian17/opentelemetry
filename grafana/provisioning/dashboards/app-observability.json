{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 1,
  "graphTooltip": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Request Rate (req/s)",
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_duration_count[5m]))",
          "legendFormat": "req/s",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "Error Rate (%)",
      "id": 2,
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_duration_count{http_status_code=~\"5.*\"}[5m])) / sum(rate(http_server_duration_count[5m])) * 100",
          "legendFormat": "error %",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Requests",
      "id": 3,
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 4,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(http_server_active_requests)",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ]
    },
    {
      "type": "stat",
      "title": "P95 Latency (ms)",
      "id": 4,
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 4,
        "h": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_duration_bucket[5m]))) * 1000",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Duration Histogram",
      "id": 5,
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_duration_bucket[5m]))) * 1000",
          "legendFormat": "p50",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(http_server_duration_bucket[5m]))) * 1000",
          "legendFormat": "p90",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_duration_bucket[5m]))) * 1000",
          "legendFormat": "p95",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_duration_bucket[5m]))) * 1000",
          "legendFormat": "p99",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            },
            "axisCenteredZero": false,
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate by Route",
      "id": 6,
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(http_server_duration_count[5m]))",
          "legendFormat": "{{http_route}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Span Rate from Tempo",
      "id": 7,
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (service_name) (rate(tempo_spanmetrics_latency_count[5m]))",
          "legendFormat": "{{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries", 
      "title": "Service Graph - Request Rate",
      "id": 8,
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (client, server) (rate(tempo_service_graph_request_total[5m]))",
          "legendFormat": "{{client}} -> {{server}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "type": "table",
      "title": "Service Dependencies",
      "id": 9,
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (client, server) (rate(tempo_service_graph_request_total[5m]))",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "indexByName": {},
            "renameByName": {
              "client": "Client Service",
              "server": "Server Service",
              "Value": "Request Rate (req/s)"
            }
          }
        }
      ]
    },
    {
      "type": "nodeGraph",
      "title": "Service Graph",
      "id": 10,
      "gridPos": {
        "x": 12,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (client, server) (rate(tempo_service_graph_request_total[5m]))",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "options": {
        "nodes": {
          "mainStatUnit": "short",
          "secondaryStatUnit": "short",
          "arcs": [
            {
              "field": "Value",
              "color": "green"
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "nodeOptions": {
              "mainStatUnit": "short",
              "secondaryStatUnit": "short",
              "arcs": [
                {
                  "field": "Value",
                  "color": "green"
                }
              ]
            }
          }
        }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Log Rate",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "expr": "sum by (level) (rate({job=\"promtail\"} | json [5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 12,
      "type": "logs",
      "title": "Recent Logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 28
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "expr": "{job=\"promtail\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true
      }
    },
    {
      "id": 13,
      "type": "traces",
      "title": "Recent Traces",
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 36
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "nativeSearch",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": [
    "opentelemetry",
    "go",
    "demo",
    "tempo",
    "service-graph"
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Go Service Observability with Service Graph",
  "version": 3
}
